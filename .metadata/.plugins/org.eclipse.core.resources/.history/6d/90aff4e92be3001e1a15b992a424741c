package positivenegative;

public class SwitchExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		 int number=20;  
	
		 switch(number){
		 case 10:
			 System.out.println("Right");  
			 break; 
		  case 20:
		    	System.out.println("Right");  
		    	break;
		  case 30: 
		    	System.out.println("Right");  
		    	break; 
		  default:
		    	System.out.println("Not in 10, 20 or 30");  
		    	break;
		 }

	}}
