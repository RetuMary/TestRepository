package positivenegative;

public class SwitchExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		 String a = " My teacher is hari";
		 
	
		 switch(a){
		 case x:
			 System.out.println("My teacher Dev");  
			 break; 
		  case y:
		    	System.out.println("My teacher is hari");  
		    	break;
		  case z: 
		    	System.out.println(" My teacher Rafsan");  
		    	break; 
		  default:
		    	System.out.println("None of them");  
		    	break;
		 }

	}}
